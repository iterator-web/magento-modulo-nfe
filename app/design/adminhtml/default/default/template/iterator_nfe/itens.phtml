var linha = null;
function carregarProduto(linhaClicada) {
    setLinha(linhaClicada);
    var produtoValue = $('produto'+linhaClicada+'value').value;
    var produtoId = produtoValue.match(/[0-9]+/g);
    if(produtoId !== null) {
        var caminhoController = '<?php echo $this->getUrl('adminhtml/nfe/buscarProduto/') ?>';
        new Ajax.Request(caminhoController,
        {
            method: 'get',
            parameters: {produto: produtoId},
            onSuccess: successFunc,
            onFailure:  failureFunc
        });
    }
}
function successFunc(xhr) {
    $('operacao'+linha+'value').setStyle({display:''});
    $('especifico'+linha+'value').setStyle({display:''});
    $('confirm_option_button'+linha+'').setStyle({display:'none'});
    
    var response = xhr.responseText.evalJSON();
    $('x_prod'+linha+'value').value = response.x_prod;
    $('c_ean'+linha+'value').value = response.gtin;
    $('c_ean_trib'+linha+'value').value = response.gtin;
    $('ncm'+linha+'value').value = response.ncm;
    $('u_com'+linha+'value').value = response.unidade;
    $('u_trib'+linha+'value').value = response.unidade;
}
function failureFunc(response){
    //alert('Erro!');
}
function setLinha(linhaClicada) {
    linha = linhaClicada;
}

function limparProduto(linhaClicada) {
   $('operacao'+linhaClicada+'value').setStyle({display:'none'});
   $('especifico'+linhaClicada+'value').setStyle({display:'none'});
   $('container'+linhaClicada+'').setStyle({display:'none'});
   $('confirm_option_button'+linhaClicada+'').setStyle({display:'block'});
   removerItem(linhaClicada);
   atualizarValorTotalProduto('operacao'+linhaClicada+'value');
   atualizarValorTotalNota();
}

function alterarOperacao(linhaSelecionada) {
    operacao = document.getElementById('operacao'+linhaSelecionada+'value').value;
    if(operacao === '') {
        $('container'+linhaSelecionada+'').setStyle({display:'none'});
    } else if(operacao === '1') {
        $('container'+linhaSelecionada+'').setStyle({display:'block'});
        $('tab_'+linhaSelecionada+'icms').setStyle({display:'block'});
        $('tab_'+linhaSelecionada+'ipi').setStyle({display:'block'});
        $('tab_'+linhaSelecionada+'ii').setStyle({display:'block'});
        $('tab_'+linhaSelecionada+'issqn').setStyle({display:'none'});
        $('panel_'+linhaSelecionada+'issqn').setStyle({display:'none'});
    } else if(operacao === '2') {
        $('container'+linhaSelecionada+'').setStyle({display:'block'});
        $('tab_'+linhaSelecionada+'icms').setStyle({display:'none'});
        $('panel_'+linhaSelecionada+'icms').setStyle({display:'none'});
        $('tab_'+linhaSelecionada+'ipi').setStyle({display:'none'});
        $('panel_'+linhaSelecionada+'ipi').setStyle({display:'none'});
        $('tab_'+linhaSelecionada+'ii').setStyle({display:'none'});
        $('panel_'+linhaSelecionada+'ii').setStyle({display:'none'});
        $('tab_'+linhaSelecionada+'issqn').setStyle({display:'block'});
    }
}

