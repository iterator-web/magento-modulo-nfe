function loadTabs(ident) {
    var tabs = new tabset('container'+ident, {
    classNames: {
      tab:        'tab',
      panel:      'panel',
      tabActive:  'selected'
    },                        
    ids: {                    
      tab:        'tab_'+ident,
      panel:      'panel_'+ident
    },                        
    onEvent:      'click',
    effects:      true
    })
};

function calendarioItens(ident) {
    Calendar.setup({
        inputField: ident+"value",
        ifFormat: "%d/%m/%y",
        showsTime: false,
        button: ident+"_trig",
        align: "Bl",
        singleClick : true
    });
}

function autocomplete(ident) {
    new Ajax.Autocompleter(
        ident,
        ident+'_search_autocomplete',
        '<?php echo $this->getUrl('adminhtml/nfe/municipiosSearch') ?>',
        {
            paramName:"query",
            minChars:2,
            updateElement:getSelectionId,
        }
    );
    function getSelectionId(li) {
        $(ident).value = li.getAttribute('url');
    }
}
